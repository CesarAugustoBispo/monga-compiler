int main()
{
  int a;
  float b;
  a = 1;
  b = 1.0;

  /* Integer operations */
  a = -a;

  a = a + 1;
  a = a - 1;
  a = a * 1;
  a = a / 1;

  a = a == a;
  a = a <= a;
  a = a >= a;
  a = a < a;
  a = a > a;

  a = !a;

  a = a && a;
  a = a || a;

  /* Floating point operations */
  b = -b;

  b = b + 1.0;
  b = b - 1.0;
  b = b * 1.0;
  b = b / 1.0;

  a = b == b;
  a = b <= b;
  a = b >= b;
  a = b < b;
  a = b > b;

  /* Mixed operations */
  b = -b;

  b = b + a;
  b = b - a;
  b = b * a;
  b = b / a;

  a = b == a;
  a = b <= a;
  a = b >= a;
  a = b < a;
  a = b > a;
}