int slow_fibonacci(int n)
{
  if ((n <= 2))
  {
    return 1;
  }

  return (slow_fibonacci((n - 1)) + slow_fibonacci((n - 2)));
}
int fast_fibonacci(int n)
{
  int prev1;
  int prev2;
  prev1 = 1;
  prev2 = 1;
  while ((n > 2))
  {
    int temp;
    temp = prev1;
    prev1 = prev2;
    prev2 = (prev1 + prev2);
    n = (n - 1);
  }
  return prev2;
}
