int slow_fibonacci(int n)
{
  if (n <= 2) { return 1; }

  return slow_fibonacci(n - 1) + slow_fibonacci(n - 2);
}

/* Speeds up fibonacci by avoiding recalculations.
 */
int fast_fibonacci(int n)
{
  int prev1 = 1;
  int prev2 = 1;

  while (n > 2)
  {
    int temp = prev1;
    prev1 = prev2;
    prev2 = prev1 + prev2;
    
    n = n - 1;
  }

  return prev2;
}